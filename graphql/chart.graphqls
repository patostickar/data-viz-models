scalar Int64

type KeyValuePair {
    key: String!
    value: Float!
}

type ChartPoint {
    timestamp: String!
    values: [KeyValuePair!]!
}

type ChartData {
    chartId: String!
    data: [ChartPoint!]!
}

type ChartDataTimestamp {
    timestamp: Int64!
    chartData: [ChartData!]!
}

type Query {
    getCharts: ChartDataTimestamp
}